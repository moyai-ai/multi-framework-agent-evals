# Google API Configuration
GOOGLE_API_KEY=your_google_api_key_here

# Model to use (default: gemini-2.0-flash)
# Options: gemini-2.0-flash, gemini-1.5-flash, gemini-1.5-pro
# Note: gemini-2.0-flash has better rate limits than gemini-2.0-flash-exp
GOOGLE_MODEL=gemini-2.0-flash

# Langfuse Configuration (for observability)
LANGFUSE_PUBLIC_KEY=pk-lf-your_public_key_here
LANGFUSE_SECRET_KEY=sk-lf-your_secret_key_here
LANGFUSE_HOST=https://cloud.langfuse.com

# Optional: Stack Exchange API (for higher rate limits)
STACKEXCHANGE_API_KEY=your_stackexchange_api_key_here

# Optional: Vertex AI Configuration (if using Vertex AI instead of Google GenAI)
GOOGLE_GENAI_USE_VERTEXAI=false
GOOGLE_CLOUD_PROJECT=your_project_id
GOOGLE_CLOUD_LOCATION=us-central1

# Optional: Debugging
DEBUG=false
LANGFUSE_DEBUG=false
LANGFUSE_TRACING_ENABLED=true
